en:
  will_paginate:
    previous_label: "&#8592; 上一页"
    next_label: "后一页 &#8594;"
    page_gap: "&hellip;"
    page_entries_info:
      single_page:
        zero:  "%{model} 没找到"
        one:   "展示 1 %{model}"
        other: "展示全部 %{count} %{model}"
      single_page_html:
        zero:  "%{model} 没找到"
        one:   "展示 <b>1</b> %{model}"
        other: "展示 <b>全部&nbsp;%{count}</b> %{model}"

      multi_page: "展示从 %{from} - %{to} 的 %{count} %{model}"
      multi_page_html: "展示 %{model} <b>%{from}&nbsp;-&nbsp;%{to}</b> 共<b>%{count}</b>记录"
    line_item:
      page_entries_info:
        single_page:
          zero:  "%{model} is empty"
          one:   "Displaying one item in your cart"
          other: "Displaying all %{count} items"
        multi_page: "Displaying items %{from} - %{to} of %{count} in total"

