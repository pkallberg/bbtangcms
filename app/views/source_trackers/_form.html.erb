<%= simple_form_for(@source_tracker) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :platform %>
    <%= f.input :from %>
    <%= f.input :version_number %>
    <%= f.input :ip %>
    <%= f.input :url %>
    <%= f.input :created_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
